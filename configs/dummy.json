{
    "logdir": "logs/",
    "out_dir":"results/",
    "out_filename": "only_fwd",
    "distributed_backend": "mpi",
    "task": "cv",
    "statistics": "cv_theta_dist_grad_norm",
    "model": "wrn_16x4_p4",
    "dataset": "cifar10",
    "trainer": {
        "type": "cv",
        "args": {
            "always_calc_grad_norm": true
        }
    },
    "bs_train": 128,
    "bs_test": 200,
    "num_data_workers": 0,
    "optimizer": {
        "type": "sgd1",
        "args": {
            "lr": 0.1,
            "weight_decay": 0.0005,
            "momentum": 0.9,
            "nesterov": false
        }
    },
    "lr_scheduler": {
        "type": "get_multi_step_lr_schedule_with_warmup",
        "args": {
            "num_warmup_steps": 5,
            "milestones": [60, 120, 160],
            "gamma": 0.2,
            "last_epoch": -1
        }
    },
    "epochs": 2,
    "steps": -1,
    "seed": 42,
    "num_chunks": 1,
    "verbose_comm": false,
    "flush_rate": -1,
    "train_batches_limit":12,
    "test_batches_limit":-1,
    "weight_stashing": true,
    "work_scheduler": "1F1B"
}
