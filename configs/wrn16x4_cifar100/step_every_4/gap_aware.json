{
    "logdir": "logs/",
    "data_dir": "/home_local/saareliad/data",
    "out_dir":"results/4partitions/wrn16x4_cifar100",
    "auto_file_name": true,
    "out_filename": "bb_pipe",
    "distributed_backend": "mpi",
    "task": "cv",
    "statistics": "cv_grad_norm",
    "model": "wrn_16x4_c100_p4",
    "dataset": "cifar100",
    "trainer": {
        "type": "cv",
        "args": {
            "always_calc_grad_norm": true
        }
    },
    "bs_train": 128,
    "bs_test": 200,
    "num_data_workers": 10,
    "optimizer": {
        "type": "sgd1",
        "args": {
            "lr": 0.4,
            "weight_decay": 0.0005,
            "momentum": 0.9,
            "nesterov": false
        }
    },
    "lr_scheduler": {
        "type": "get_multi_step_lr_schedule_with_warmup",
        "args": {
            "num_warmup_steps": 5,
            "milestones": [60, 120, 160],
            "gamma": 0.2,
            "last_epoch": -1
        }
    },
    "weight_prediction": {
        "type": "msnag",
        "args": {
            "pred_mem": "clone",
            "nag_with_predictor": true
        }
    },
    "gap_aware": {
        "type": "sgd1",
        "policy": "almost_last_partition",
        "args": {
            "big_gamma": 0.999,
            "epsilon": 1e-8,
            "penatly_for_weight_decay": true
        }
    },
    "epochs": 400,
"step_every": 4,
"cudnn_benchmark": true,
    "steps": -1,
    "seed": 42,
    "num_chunks": 1,
    "verbose_comm": false,
    "flush_rate": -1,
    "seed_from_cmd": true,
    "stage_to_device_map": [0, 0, 0, 0]

}
