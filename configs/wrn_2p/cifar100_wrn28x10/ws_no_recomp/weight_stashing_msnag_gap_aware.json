{
    "logdir": "logs/",
    "out_dir":"results/2p_ga_study",
    "auto_file_name": true,
    "out_filename": "ninja_norecomp",
    "distributed_backend": "mpi",
    "task": "cv_sep",
    "statistics": "cv",
    "model": "wrn_28x10_c100_dr03_p2",
    "dataset": "cifar100",
    "trainer": {
        "type": "cv",
        "args": {
            "always_calc_grad_norm": false
        }
    },
    "bs_train": 128,
    "bs_test": 200,
    "num_data_workers": 10,
    "optimizer": {
        "type": "sgd1",
        "args": {
            "lr": 0.1,
            "weight_decay": 0.0005,
            "momentum": 0.9,
            "nesterov": false
        }
    },
    "lr_scheduler": {
        "type": "get_multi_step_lr_schedule_with_warmup",
        "args": {
            "num_warmup_steps": 5,
            "milestones": [60, 120, 160],
            "gamma": 0.2,
            "last_epoch": -1
        }
    },
    "weight_prediction": {
        "type": "msnag",
        "args": {
            "pred_mem": "clone",
            "nag_with_predictor": true
        }
    },
    "gap_aware": {
        "type": "sgd1",
        "policy": "all_except_last",
        "args": {
            "big_gamma": 0.999,
            "epsilon": 1e-8
        }
    },
    "epochs": 205,
    "steps": -1,
    "seed": 42,
    "num_chunks": 1,
    "verbose_comm": false,
    "flush_rate": -1,
    "weight_stashing": true,
    "work_scheduler": "1F1B",
    "seed_from_cmd": true,
    "max_buffers": 1,
    "cudnn_benchmark": true,
    "keep_buffers_alive": true,
    "no_recomputation": true,
    "nesterov_set_for_last_partition": true

}
