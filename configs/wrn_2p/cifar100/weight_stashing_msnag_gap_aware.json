{
    "logdir": "logs/",
    "out_dir":"results/",
    "out_filename": "ninja4",
    "auto_file_name": true,
    "distributed_backend": "mpi",
    "task": "cv",
    "statistics": "cv_theta_dist_grad_norm",
    "model": "wrn_16x4_c100_p2",
    "dataset": "cifar100",
    "trainer": {
        "type": "cv",
        "args": {
            "always_calc_grad_norm": true
        }
    },
    "bs_train": 128,
    "bs_test": 200,
    "num_data_workers": 6,
    "optimizer": {
        "type": "sgd1",
        "args": {
            "lr": 0.1,
            "weight_decay": 0.0005,
            "momentum": 0.9,
            "nesterov": false
        }
    },
    "lr_scheduler": {
        "type": "get_multi_step_lr_schedule_with_warmup",
        "args": {
            "num_warmup_steps": 5,
            "milestones": [60, 120, 160],
            "gamma": 0.2,
            "last_epoch": -1
        }
    },
    "weight_prediction": {
        "type": "msnag",
        "args": {
            "pred_mem": "clone",
            "nag_with_predictor": true
        }
    },
    "gap_aware": {
        "type": "sgd1",
        "policy": "all_except_last",
        "args": {
            "big_gamma": 0.999,
            "epsilon": 1e-8
        }
    },
    "epochs": 200,
    "steps": -1,
    "seed": 42,
    "num_chunks": 1,
    "verbose_comm": false,
    "flush_rate": -1,
    "weight_stashing": true,
    "work_scheduler": "1F1B",
    "seed_from_cmd": true
}
