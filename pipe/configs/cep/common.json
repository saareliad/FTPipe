{
    "logdir": "logs/cep/",
    "data_dir": "/home_local/saareliad/data",
    "out_dir": "results/cep",
    "auto_file_name": true,
    "out_filename": "cep",
    "distributed_backend": "mpi",
    "model": "cep_netN50_C500_4p_bw12_metis",
    "stage_to_device_map": [0, 0, 0, 0, 1, 2, 3],
    "nprocs": 7,
    "dataset": "cep",
    "trainer": {
        "type": "cep",
        "args": {
            "always_calc_grad_norm": false
        }
    },
    "statistics": "cv",
    "step_every": 1,
    "bs_train": 256,
    "bs_test": 256,
    "num_data_workers": 5,
    "optimizer": {
        "type": "adamw",
        "args": {
            "lr": 0.001,
            "weight_decay": 1e-2,
        }
    },
    "lr_scheduler": {
        "type": "get_constant_schedule_with_warmup",
        "preproc_args": {
        },
        "args": {
