            "last_epoch": -1
        }
    },
    "epochs": -1,
    "steps": 10000,
    "seed_from_cmd": false,
    "seed": 42,
    "bs_train_from_cmd": false,
    "bs_test_from_cmd": false,
    "num_chunks": 1,
    "verbose_comm": false,
    "flush_rate": -1,
    "work_scheduler": "gpipe",
    "cudnn_benchmark": true,
    "max_buffers": 1,
    "keep_buffers_alive": false,
    "train_batches_limit": -1,
    "log_frequency": 200,
    "dont_drop_last": true,
    "test_batches_limit":-1
