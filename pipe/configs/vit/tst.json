{
  "base_config_path": [
    "cifar100.json",
    "vit_large_patch32_384_in21k.json"
  ],
  "base_config_path_is_relative": true,
  "bs_train": 32,
  "bs_test": 32,
  "num_data_workers": 10,
  "dont_drop_last": true,
  "step_every": 1,
  "optimizer": {
    "type": "sgd1",
    "args": {
      "lr": 0.1,
      "weight_decay": 0.0005,
      "momentum": 0.9,
      "nesterov": true
    }
  }
  "lr_scheduler": {
    "type": "get_constant_schedule_with_warmup",
    "preproc_args": {
    },
    "args": {
      "num_warmup_steps": 200,
      "last_epoch": -1
    }
  },
  "weight_stashing": false,
  "work_scheduler": "1F1B",
  "cudnn_benchmark": true,
  "seed_from_cmd": false,
  "seed": 42
}